   set clk_candidates {0 INOUT clk clk 0 IN clk clk 0 IN weight_in_val weight_in_val 0 IN weight_in_msg weight_in_msg 0 IN act_in_val act_in_val 0 IN act_in_msg act_in_msg 0 IN accum_in_val accum_in_val 0 IN accum_in_msg accum_in_msg 0 IN act_out_rdy act_out_rdy 0 IN accum_out_rdy accum_out_rdy 0 IN weight_out_rdy weight_out_rdy} 
   set i_candidates {0 IN weight_in_val weight_in_val 0 IN weight_in_msg weight_in_msg 0 IN act_in_val act_in_val 0 IN act_in_msg act_in_msg 0 IN accum_in_val accum_in_val 0 IN accum_in_msg accum_in_msg 0 IN act_out_rdy act_out_rdy 0 IN accum_out_rdy accum_out_rdy 0 IN weight_out_rdy weight_out_rdy} 
   set o_candidates {0 OUT weight_in_rdy weight_in_rdy 0 OUT act_in_rdy act_in_rdy 0 OUT accum_in_rdy accum_in_rdy 0 OUT act_out_val act_out_val 0 OUT act_out_msg act_out_msg 0 OUT accum_out_val accum_out_val 0 OUT accum_out_msg accum_out_msg 0 OUT weight_out_val weight_out_val 0 OUT weight_out_msg weight_out_msg} 
   foreach { orsid orsmode iclk ote } $clk_candidates {
   puts "Timing reporting for orsid=$orsid orsmode=$orsmode iclk=$iclk ote=$ote "
      foreach { irsid irsmode iport ite } $i_candidates {
